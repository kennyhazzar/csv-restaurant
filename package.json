{
  "name": "csv-restaurant",
  "version": "1.0.0",
  "description": "A web application that allows you to generate a menu from a csv file",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "csv",
    "restaurant",
    "web-app",
    "menu"
  ],
