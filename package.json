  "license": "ISC",
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "axios": "^0.21.1",
    "concurrently": "^6.1.0",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.12.9",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "react-router-dom": "^5.2.0",
    "shortid": "^2.2.16"
  }
}
